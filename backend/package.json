{
  "name": "clausify-backend",
  "version": "0.1.0",
  "private": true,
  "description": "FastAPI backend for Clausify application",
  "scripts": {
    "dev": "python -m uvicorn main:app --reload --host 127.0.0.1 --port 8000",
    "start": "python -m uvicorn main:app --host 127.0.0.1 --port 8000",
    "build": "echo 'FastAPI backend - no build step required'",
    "test": "pytest",
    "lint": "flake8 . --count --select=E9,F63,F7,F82 --show-source --statistics",
    "format": "black .",
    "install-deps": "pip install -r requirements.txt",
    "freeze-deps": "pip freeze > requirements.txt"
  },
  "keywords": [
    "fastapi",
    "python",
    "backend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
